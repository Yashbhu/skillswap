generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             Int     @id @default(autoincrement())
  username       String
  email          String  @unique
  status         String
  totalBookings  Int     @default(0)
  pending        Int     @default(0)
  completed      Int     @default(0)
}
